@model IEnumerable<MMO_Mania.Models.LeaderboardModel>

@{
    ViewBag.Title = "Ranking";
}

<h1>Rankings</h1>



<table class="table">
    <tr>
      
        <th>
            @Html.DisplayNameFor(model => model.Char_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Level)
        </th>
        
        <th>
            @Html.DisplayNameFor(model => model.Ranking)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
      
        <td>
            @Html.DisplayFor(modelItem => item.Char_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Level)
        </td>
        
        <td>
            @Html.DisplayFor(modelItem => item.Ranking)
        </td>
     
    </tr>
}

</table>
